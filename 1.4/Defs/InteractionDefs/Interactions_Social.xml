<?xml version="1.0" encoding="UTF-8"?>
<Defs>
    <InteractionDef>
        <defName>VFEE_RoyalGossip</defName>
        <label>royal gossip</label>
        <workerClass>VFEEmpire.InteractionWorker_RoyalGossip</workerClass>
        <symbol>UI/RoyalGossip</symbol>
        <initiatorXpGainSkill>Social</initiatorXpGainSkill>
        <initiatorXpGainAmount>10</initiatorXpGainAmount>
        <logRulesInitiator>
          <rulesStrings>
            <li>r_logentry->[INITIATOR_nameDef] and [RECIPIENT_nameDef] [talkedabout] [THIRDPARTY_nameDef].</li>
            <li>r_logentry->[INITIATOR_nameDef] [talkedabout] [THIRDPARTY_nameDef] with [RECIPIENT_nameDef].</li>
    
            <li>talkedabout(p=4)->chatted about</li>
            <li>talkedabout->shared a word about</li>
            <li>talkedabout->spoke about</li>
            <li>talkedabout->gabbed about</li>
            <li>talkedabout->talked about</li>
            <li>talkedabout->joked about</li>
            <li>talkedabout->quipped about</li>
    
          </rulesStrings>
        </logRulesInitiator>
      </InteractionDef>
</Defs>